.body {
    display: flex;
    flex-direction: column;
    justify-content: center;
    align-items: center;
  }
  
  .body .noneImg {
    width: 200rpx;
    height: 200rpx;
  }

  .body .administer {
    width: 85%;
    padding: 35rpx 25rpx;
    background-color: #fff;
    box-shadow: 0 0 20rpx #a1a1a1;
    margin: 0 auto;
    margin-top: 20rpx;
    display: flex;
    flex-direction: column;
    border-radius: 20rpx;
  }

 .body .administer .top{
    display: flex;
    border-radius:10rpx ;
    align-items: center;
}

.body .administer .buttom .refuse{
    width: 300rpx;
    background-color: rgba(255, 102,102, .2);
    color: rgb(255, 102, 102);
}

  .body .add {
    margin-top: 100rpx;
    width: 400rpx;
    background-color: rgba(10, 149, 241, 0.2);
    color: rgb(10, 149, 241);
}